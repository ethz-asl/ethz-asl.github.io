# Gemfile for ASL datasets Jekyll site

source "https://rubygems.org"

# Use the GitHub Pages gem so your local build matches what GitHub runs
gem "github-pages", group: :jekyll_plugins

# Needed when using Ruby 3.x (Jekyll still expects WEBrick)
gem "webrick", "~> 1.8"

group :development do
  # Optional: nice error pages & console
  gem "jekyll-admin", "~> 0.11", require: false
end
